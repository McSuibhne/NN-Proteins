OBJECTST = Layer.o MModel.o NN.o Predict_thresholds.o
HEADERST = Layer.h MModel.h NN.h Sequence.h 

CC	= g++
CFLAGS  = -O3

Predict_thresholds :	$(OBJECTST) 
	$(CC) $(CFLAGS) -o $@ $(OBJECTST) $(LLIBS)

Predict_thresholds.o : Predict_thresholds.cxx $(HEADERST)
	$(CC) -c $(CFLAGS) $*.cxx

Layer.o : Layer.cxx $(HEADERST)
	$(CC) -c $(CFLAGS) $*.cxx

MModel.o : MModel.cxx $(HEADERST)
	$(CC) -c $(CFLAGS) $*.cxx

NN.o : NN.cxx $(HEADERST)
	$(CC) -c $(CFLAGS) $*.cxx


clean:
	rm *.o Predict_thresholds
